    "@aws-sdk/client-ec2": "3.621.0",
    "@aws-sdk/client-ecs": "3.621.0",
    "@aws-sdk/client-eks": "3.621.0",
    "@aws-sdk/client-elastic-load-balancing": "3.621.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "3.621.0",
