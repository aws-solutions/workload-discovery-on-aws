{
  "name": "workload-discovery-ui",
  "version": "v2.3.4",
  "private": true,
  "description": "UI code for the Workload Discovery solution",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-amplify/core": "5.8.6",
    "@aws-sdk/util-utf8-browser": "^3.259.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.0.8",
    "amplify-graphql-docs-generator": "^2.2.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint": "^8.44.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "history": "^4.10.1",
    "jsdom": "^22.1.0",
    "msw": "^2.4.9",
    "node-fetch": "^3.3.1",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.56.1",
    "pngjs": "^7.0.0",
    "prettier": "^3.0.0",
    "vite": "^6.2.1",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-plugin-svgr": "4.3.0",
    "vitest": "^3.1.1",
    "vitest-browser-react": "^0.1.1"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "5.3.2",
    "@cloudscape-design/collection-hooks": "1.0.69",
    "@cloudscape-design/components": "3.0.944",
    "@cloudscape-design/design-tokens": "3.0.55",
    "@cloudscape-design/global-styles": "1.0.40",
    "@uiw/react-json-view": "1.12.2",
    "aws-amplify": "5.3.12",
    "crypto-js": "4.2.0",
    "currency-symbol-map": "5.0.1",
    "cytoscape": "3.33.0",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-expand-collapse": "4.1.0",
    "cytoscape-fcose": "2.1.0",
    "cytoscape-grid-guide": "2.3.3",
    "dayjs": "1.8.28",
    "file-saver": "2.0.2",
    "filefy": "0.1.10",
    "js-file-download": "0.4.12",
    "lodash.forown": "4.4.0",
    "lodash.isobject": "3.0.2",
    "object-hash": "3.0.0",
    "p-limit": "4.0.0",
    "ramda": "0.28.0",
    "randomcolor": "0.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-papaparse": "3.11.1",
    "react-query": "^3.39.1",
    "react-router-dom": "5.3.4",
    "react-use": "^17.4.0",
    "sass": "1.38.0",
    "svgcanvas": "2.5.0",
    "ua-parser-js": "1.0.35",
    "valid-filename": "4.0.0"
  },
  "overrides": {
    "cookie": "0.7.0",
    "axios": "1.12.0"
  },
  "scripts": {
    "start": "vite",
    "build": "npm run clean && npm i && NODE_OPTIONS=--max_old_space_size=4096 vite build && npm run build:zip && npm run build:dist",
    "serve": "vite preview",
    "pretest": "npm i && rm -rf coverage && mkdir coverage",
    "test": "vitest run --coverage --passWithNoTests --silent passed-only",
    "test:ci": "npm ci && npx playwright install --with-deps && npm run test",
    "test:watch": "vitest --coverage --passWithNoTests",
    "build:zip": "cd dist && zip -rq --exclude=settings.js ../ui.zip . && cd ..",
    "lint": "eslint",
    "clean": "rm -rf build",
    "build:dist": "mv ui.zip dist/",
    "graphql-gen": "npm i -save-dev amplify-graphql-docs-generator && amplify-graphql-docs-generator --schema ../backend/graphql/schema/perspective-api.graphql --output src/API/GraphQL/ --language javascript --separateFiles true"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-jsx",
      "@babel/plugin-transform-runtime",
      [
        "transform-imports",
        {
          "@awsui": {
            "transform": "@awsui",
            "preventFullImport": true
          },
          "cytoscape-react": {
            "transform": "*",
            "preventFullImport": true
          }
        }
      ]
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
