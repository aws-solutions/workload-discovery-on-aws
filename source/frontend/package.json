{
  "name": "workload-discovery-ui",
  "version": "2.0.0",
  "private": true,
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@emotion/react": "^11.9.3",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.29.2",
    "amplify-graphql-docs-generator": "^2.2.4",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-imports": "^2.0.0",
    "esbuild": "^0.16.17",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.0.0",
    "prettier": "^2.8.3",
    "typescript": "^3.8.3",
    "vite": "^4.1.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^0.29.3"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "3.5.7",
    "@awsui/collection-hooks": "1.0.22",
    "@awsui/components-react": "3.0.655",
    "@awsui/design-tokens": "3.0.23",
    "@awsui/global-styles": "1.0.16",
    "aws-amplify": "4.3.37",
    "currency-symbol-map": "5.0.1",
    "cytoscape": "3.21.0",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-expand-collapse": "4.1.0",
    "cytoscape-fcose": "2.1.0",
    "cytoscape-grid-guide": "2.3.3",
    "dayjs": "1.8.28",
    "file-saver": "2.0.2",
    "filefy": "0.1.10",
    "js-file-download": "0.4.12",
    "lodash.forown": "4.4.0",
    "lodash.isobject": "3.0.2",
    "object-hash": "2.0.2",
    "ramda": "0.28.0",
    "randomcolor": "0.5.4",
    "react": "17.0.2",
    "react-cytoscapejs": "1.2.1",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-json-view": "1.21.1",
    "react-papaparse": "3.11.1",
    "react-query": "^3.39.1",
    "react-router-dom": "5.2.0",
    "react-use": "^17.4.0",
    "sass": "1.38.0",
    "ua-parser-js": "0.7.33",
    "valid-filename": "3.1.0"
  },
  "scripts": {
    "start": "vite",
    "build": "npm run clean && npm i && NODE_OPTIONS=--max_old_space_size=4096 vite build && npm run build:zip && npm run build:dist",
    "serve": "vite preview",
    "pretest": "npm i",
    "test": "vitest run --coverage --passWithNoTests",
    "test:ci": "npm ci && vitest run --coverage --allowOnly=false --passWithNoTests",
    "test:watch": "vitest --coverage --passWithNoTests",
    "build:zip": "cd dist && zip -rq --exclude=settings.js ../ui.zip . && cd ..",
    "lint": "eslint",
    "clean": "rm -rf build",
    "build:dist": "mv ui.zip dist/",
    "graphql-gen": "npm i -save-dev amplify-graphql-docs-generator && amplify-graphql-docs-generator --schema ../backend/graphql/schema/perspective-api.graphql --output src/API/GraphQL/ --language javascript --separateFiles true"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-jsx",
      "@babel/plugin-transform-runtime",
      [
        "transform-imports",
        {
          "@awsui": {
            "transform": "@awsui",
            "preventFullImport": true
          },
          "cytoscape-react": {
            "transform": "*",
            "preventFullImport": true
          }
        }
      ]
    ]
  }
}
